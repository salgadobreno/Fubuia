<%= form_tag(import_event_admin_events_path, :method => :post) do %>
  <div>
    <label>
      Facebook Event FID or URL:
      <%= text_field_tag "fid", params[:fid] %>
    </label>
    <strong>OR</strong>
    <label>
      List of FIDs/URLs
      <%= text_area_tag "fids" %>
    </label>
  </div>

  <div>
    <%= submit_tag "Submit" %>
  </div>
<% end %>
